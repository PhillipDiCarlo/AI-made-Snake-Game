{
    "version": "0.2.0",
    "configurations": [
      {
        "name": "Run Snake Game",
        "type": "python",
        "request": "launch",
        "program": "${workspaceFolder}/main.py",
        "console": "integratedTerminal",
        "cwd": "${workspaceFolder}",
        "env": {
          "PYTHONPATH": "${workspaceFolder}"
        },
        "envFile": "${workspaceFolder}/.env",
        "python": "${workspaceFolder}/.venv/Scripts/python.exe",
        "args": [],
        "justMyCode": true,
        "stopOnEntry": false,
        "showReturnValue": true,
        "logToFile": false
      }
    ]
  }
  